{
  "name": "gdi-documentation-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "node dev.js",
    "start": "concurrently \"npm run start:landing\" \"npm run start:user-portal\" \"npm run start:cms\" \"npm run start:daams\" \"npm run start:system-admin\" \"npm run start:developer\"",
    "start:landing": "cd apps/landing && npm start",
    "start:user-portal": "cd apps/user-portal && npm start",
    "start:cms": "cd apps/cms && npm start",
    "start:daams": "cd apps/daams && npm start",
    "start:system-admin": "cd apps/system-admin && npm start",
    "start:developer": "cd apps/developer && npm start",
    "build": "npm run build:landing && npm run build:user-portal && npm run build:cms && npm run build:daams && npm run build:system-admin && npm run build:developer",
    "build:landing": "cd apps/landing && npm run build",
    "build:user-portal": "cd apps/user-portal && npm run build",
    "build:cms": "cd apps/cms && npm run build",
    "build:daams": "cd apps/daams && npm run build",
    "build:system-admin": "cd apps/system-admin && npm run build",
    "build:developer": "cd apps/developer && npm run build",
    "serve": "concurrently \"npm run serve:landing\" \"npm run serve:user-portal\" \"npm run serve:cms\" \"npm run serve:daams\" \"npm run serve:system-admin\" \"npm run serve:developer\"",
    "serve:landing": "cd apps/landing && npm run serve",
    "serve:user-portal": "cd apps/user-portal && npm run serve",
    "serve:cms": "cd apps/cms && npm run serve",
    "serve:daams": "cd apps/daams && npm run serve",
    "serve:system-admin": "cd apps/system-admin && npm run serve",
    "serve:developer": "cd apps/developer && npm run serve",
    "install:all": "npm install && npm run install:apps",
    "install:apps": "node dev.js install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0"
  }
}
